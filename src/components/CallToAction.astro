---

import CtaTddExercise from "./CtaTddExercise.html";
import CtaNewsletter from "./CtaNewsletter.astro";

function findCtaContent(ctaString) {
    switch (ctaString) {
        case "CtaTddExercise":
            return CtaTddExercise;
        case "CtaNewsletter":
            return CtaNewsletter;
        default:
            return undefined;
    }
}

const CtaContent = findCtaContent(Astro.props.cta);
---

{CtaContent && <CtaContent/>}
